package Keyword;

public class This {
	
	
	int i;
	String str;
	
	public void display(int i, String str) {
		
		this.i=i;
		this.str=str;
		System.out.println(i+" "+str);
		
	}
	
	public void show() {
		
		System.out.println(i+" 'Instance Variables' "+str);
	}

	public static void main(String[] args) {

      This thi=new This();
      thi.display(10, "Tom");
      thi.show();
      
      
		
		
		
	}

}
