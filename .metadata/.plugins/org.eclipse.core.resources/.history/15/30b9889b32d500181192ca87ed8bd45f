		} catch (InterruptedException e1) {
			// TODO Auto-generated catch block
			e1.printStackTrace();
		}
		System.out.println("JAVA");
		
		//2.
		
		
			
			try {
				FileInputStream fi=new FileInputStream("C:\\Users\\gupta.av\\eclipse-workspace\\0.1Automation\\src\\main\\java\\Exception");
			} catch (FileNotFoundException e) {
